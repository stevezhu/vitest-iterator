{
  "name": "vitest-iterator",
  "version": "0.0.1",
  "description": "Additional vitest matchers for iterators and generators",
  "keywords": [
    "vitest",
    "generator",
    "iterator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stevezhu/vitest-iterator.git"
  },
  "license": "MIT",
  "author": "Steve Zhu <st.zhu1@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./assertion": {
      "types": "./assertion.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assertion.d.ts"
  ],
  "scripts": {
    "build": "nps build",
    "format": "nps prettier.fix",
    "lint": "concurrently 'nps prettier' 'nps eslint'",
    "prepare": "nps build",
    "test": "concurrently 'vitest run' 'vitest typecheck --run'",
    "test:coverage": "vitest --coverage",
    "test:typecheck": "vitest typecheck",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.25.21"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nps": "^5.10.0",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.2",
    "prettier-plugin-tsconfig": "^0.0.1",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "peerDependencies": {
    "vitest": ">=0.27"
  },
  "packageManager": "pnpm@7.28.0",
  "engines": {
    "node": ">=18.14.0"
  }
}
